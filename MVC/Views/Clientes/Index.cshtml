@model IEnumerable<MVC.Models.Cliente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataNascFund)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RendaLucro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sexo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Endereco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Documento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoEmpresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodTipoCliNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataNascFund)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RendaLucro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sexo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Endereco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Documento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoEmpresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodTipoCliNavigation.NomeTipoCli)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CodCli">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CodCli">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CodCli">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
